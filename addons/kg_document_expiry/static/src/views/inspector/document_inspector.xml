<?xml version="1.0" encoding="UTF-8"?>

<templates id="documents.template" xml:space="preserve">
    <t t-name="documents.DocumentsInspector.documentsInfo" t-inherit="documents.DocumentsInspector.documentsInfo"
       t-inherit-mode="extension" owl="1">
        <xpath expr="//table[tbody]" position="inside">
                <t t-call="documents.DocumentsInspector.field">
                        <t t-set="field">expiry_date</t>
                    <t t-set="additionalProps"/>
                </t>
             <t t-call="documents.DocumentsInspector.field">
                        <t t-set="field">document_type</t>
                    <t t-set="additionalProps"/>
                </t>
        </xpath>
    </t>
</templates>
