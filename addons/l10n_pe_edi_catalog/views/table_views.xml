<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
	<!-- Tabla 25 - Convenios para evitar la doble tributación -->
	<record id="view_table_25_tree" model="ir.ui.view">
		<field name="name">Tabla 25 - Convenios para evitar la doble tributación</field>
		<field name="model">l10n_pe_edi.table.25</field>
		<field name="arch" type="xml">
			<tree string="Convenios para evitar la doble tributación">
				<field name="code" />
				<field name="name" />
				<field name="active" />
			</tree>
		</field>
	</record>
	<record id="view_table_25_form" model="ir.ui.view">
		<field name="name">Tabla 25 - Convenios para evitar la doble tributación</field>
		<field name="model">l10n_pe_edi.table.25</field>
		<field name="arch" type="xml">
			<form string="Convenios para evitar la doble tributación">
				<group>
					<group>
						<field name="code" />
						<field name="name" />
						<field name="active" />
					</group>
				</group>
			</form>
		</field>
	</record>
	<record model="ir.actions.act_window" id="action_table_25">
		<field name="name">Tabla 25 - Convenios para evitar la doble tributación</field>
		<field name="res_model">l10n_pe_edi.table.25</field>
		<field name="view_mode">tree,form</field>
	</record>

