<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
        <record id="ple_inv_bal_line_initial_balances_08_form_view" model="ir.ui.view">
        <field name="name">ple.inv.bal.line.initial.balances.08.form.view</field>
        <field name="model">ple.inv.bal.line.initial.balances.08</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <field name="catalog_code"/>
                        <field name="type_document_transmitter"/>
                        <field name="state"/>
                        <field name="free_camp"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

    <record id="ple_inv_bal_line_initial_balances_08_tree_view" model="ir.ui.view">
    <field name="name">ple.inv.bal.line.initial.balances.08.tree.view</field>
    <field name="model">ple.inv.bal.line.initial.balances.08</field>
        <field name="arch" type="xml">
            <tree create="false">
                <field name="transmitter_name" width="30%"/>
                <field name="total_title_costs" width="30%"/>
                <field name="total_title_provision" width="30%"/>
            </tree>
        </field>
    </record>

</odoo>